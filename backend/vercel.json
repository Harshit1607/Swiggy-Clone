{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "cart/(.*)", "dest": "server.js" },
    { "src": "orders/(.*)", "dest": "server.js" },
    { "src": "payment/(.*)", "dest": "server.js" },
    { "src": "restaurants/(.*)", "dest": "server.js" },
    { "src": "user/(.*)", "dest": "server.js" }
  ],
  "env": {
    "VERCEL_DEV_PORT": "5000"
  }
}
